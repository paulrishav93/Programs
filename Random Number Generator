#include <iostream>
#include <stdlib.h>
#include <time.h>

using namespace std;

int main()
{
    srand(time(0));
    
    const int iterations=rand()%10;
    const int size=rand()%10;

    for(int i=0;i<iterations;++i)
    {
        for(int j=0;j<size;++j)
            cout<<rand()%100<<" ";

        cout<<endl;
    }

    return 0;
}
